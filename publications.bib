  keywords        = {Biopsy; Carcinoma, pathology; Carcinoma, Ductal, pathology; Consensus; Humans; Male; Neoplasm Grading, standards; Neoplasm Invasiveness; Pathology, Clinical, standards; Predictive Value of Tests; Prostatic Neoplasms, pathology; Urology, standards},
  nlm-id          = {7707904},
